{
  "include": ["src"],
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "declaration": false,
    "module": "Node16",
    "target": "es2020",
    "moduleResolution": "Node16",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
